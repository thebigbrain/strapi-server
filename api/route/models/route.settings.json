{
  "connection": "default",
  "collectionName": "routes",
  "info": {
    "name": "route"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "icon": {
      "type": "string"
    },
    "path": {
      "type": "string",
      "default": "/"
    },
    "name": {
      "type": "string",
      "default": "根路径"
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "parent": {
      "model": "route"
    },
    "redirect": {
      "type": "string"
    },
    "exact": {
      "type": "boolean",
      "default": false
    },
    "ui": {
      "model": "ui-component"
    }
  }
}
